// Копает прямо 
$coords = RND(1,10) 
$x = 703 + $coords
$y = 488 + $coords
$delay = RND(50,150)
WAITMS($delay)
MOVE($x, $y)
WAITMS($delay)
MOVE($x, $y)
WAITMS($delay)
LCLICK($x, $y)
WAITMS($delay)
MOVE($x, $y)
WAITMS($delay)
MOVE($x-3, $y)
WAITMS($delay)
KEYDOWN(#M)
WAITMS(50)
KEYUP(#M)
WAITMS(22000-$delay)
