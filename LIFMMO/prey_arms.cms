#name "Молитва + строевая"

// begin your script here

$cd = 30
$lag_wait = 0.5
$do_wait = 5.5
$cycles = 2
SUB(arm, $key)
   	KEYPRESS(#R)
		wait($lag_wait)
		KEYPRESS($key)
		wait($lag_wait)
		KEYPRESS(#R)
		wait($lag_wait)
END_SUB

SUB(f8, $tryes)
  FOR($try=0, $try<$tryes)
  	arm(#F9)
  END_CYC
  wait($lag_wait)
END_SUB


f8(1) 
KEYPRESS(#E)
$cast = 10 
wait($cast) 
