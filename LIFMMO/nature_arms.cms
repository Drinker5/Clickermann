#name "Природа + строевая"

// begin your script here

$cd = 30
$lag_wait = 0.5
$do_wait = 6
$cycles = 5
KEYPRESS(#R)
wait($lag_wait)
KEYPRESS(#7)
wait($lag_wait)
KEYPRESS(#R)
wait($lag_wait)

FOR($i=0, $i < $cycles)
  KEYPRESS(#E)
	wait($do_wait)
END_CYC

$wait($cd-$do_wait*$cycles)


//halt // for single runq